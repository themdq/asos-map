import WeatherStationsMap from '../components/WeatherStationsMap';
import { QueryClientProvider } from '@tanstack/react-query';
import { queryClient } from '@/lib/queryClient';
---

<MainLayout content={{ title: 'ASOS Stations' }}>
  <QueryClientProvider client={queryClient}>
    <WeatherStationsMap client:load />
  </QueryClientProvider>
</MainLayout>